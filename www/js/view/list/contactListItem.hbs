{{#.}}
<li class="list-group-title">{{letter}}</li>
	{{#each list}}
		<li data-id="{{_id}}">
			<a href="html/contact.html?id={{_id}}" class="item-link">
				<div class="item-content">
					<div class="item-media">
					    {{#if picture.thumbnail}}
                            <img src="{{picture.thumbnail}}">
                        {{else}}
                            <img src="img/contact.jpg" width="44">
                        {{/if}}
					</div>
					<div class="item-inner">
						<div class="item-title-row">
							<div class="item-title">{{name.first}} {{name.last}}</div>
						</div>
						<div class="item-subtitle">{{name.title}}</div>
					</div>
				</div>
			</a>
		</li>
	{{/each}}
{{/.}}